   <!-- find Resources by accountId -->
   <select id="getAccountResources" parameterType="String" resultType="com.gionee.baserom.search.pojo.Resources">
		SELECT 
		<include refid="Base_Column_List" />
		FROM resources WHERE id in(
			SELECT res_id FROM
			resources_role WHERE role_id in(
				SELECT role_id FROM account_role WHERE account_id=#{accountId}
			)
		)
	</select>
	
	<select id="queryByRoleId" parameterType="Integer" resultType="com.gionee.baserom.search.pojo.Resources">
		select r.* from resources r
		left join resources_role rr on rr.res_id = r.id
		where rr.role_id=#{roleId}
	</select>
	
	<select id="queryAccountMenu" parameterType="Integer" resultMap="BaseResultMap">
		select res.* from resources res
		left join resources_role r on r.res_id = res.id
		left join account a on a.group_id = r.role_id
		where a.id=#{id} and res.type!=3 order by parent_id_list,id
	</select>
	
	<select id="queryAccountOpt" parameterType="Integer" resultMap="BaseResultMap">
		select res.* from resources res
		left join resources_role r on r.res_id = res.id
		left join account a on a.group_id = r.role_id
		where a.id=#{accId} and res.parent_id=#{resId} and res.type=3 order by parent_id_list,id
	</select>
	
	
